                        android:layout_height="0dp"
                        android:gravity="center"
                        android:text="SETTINGS"
                        android:layout_weight="2"
                        android:lines="1"
                        android:textSize="@dimen/d10"
                        android:textColor="#FFF" />
                </LinearLayout>
            </FrameLayout>


