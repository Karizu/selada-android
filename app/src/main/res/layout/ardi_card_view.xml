                    android:layout_marginTop="@dimen/d8"
                    android:layout_marginBottom="@dimen/d12"
                    android:gravity="center"
                    android:weightSum="5">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="3"
                        android:scaleType="fitCenter"
                        android:src="@drawable/top_history_icon"
                        android:layout_marginBottom="4dp"/>

                    <com.boardinglabs.mireta.selada.component.fontview.RobotoLightTextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:gravity="center"
                        android:text="HISTORY"
                        android:layout_weight="2"
                        android:lines="1"
                        android:textSize="@dimen/d10"
                        android:textColor="#FFF" />
                </LinearLayout>
            </FrameLayout>
            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingBottom="@dimen/d16">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bcg_menu_atas" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_marginTop="@dimen/d8"
                    android:layout_marginBottom="@dimen/d12"
                    android:gravity="center"
                    android:weightSum="5">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="3"
                        android:scaleType="fitCenter"
                        android:src="@drawable/top_reward_icon"
                        android:layout_marginBottom="4dp" />

                    <com.boardinglabs.mireta.selada.component.fontview.RobotoLightTextView
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:gravity="center"
                        android:text="POINT"
                        android:layout_weight="2"
                        android:lines="1"
                        android:textSize="@dimen/d10"
                        android:textColor="#FFF" />
                </LinearLayout>
            </FrameLayout>
            <FrameLayout
                android:id="@+id/menuSetting"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingBottom="@dimen/d16">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bcg_menu_atas" />

                <LinearLayout
                    android:layout_width="match_parent"
